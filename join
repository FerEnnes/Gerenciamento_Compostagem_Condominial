--Consulta com INNER JOIN

SELECT MORADORES.nome, RESIDUOS.qtDoada, RESIDUOS.dataRecebimento
FROM MORADORES
INNER JOIN RESIDUOS ON MORADORES.idMorador = RESIDUOS.idmorador;

--Consulta com LEFT JOIN

SELECT MATERIAIS.*, Utilizados.*
FROM MATERIAIS
LEFT JOIN Utilizados ON MATERIAIS.codEentrega = Utilizados.codEentrega;

--Consulta com RIGHT JOIN

SELECT COMPOSTEIRAS.*, COMPOSTO_LIQUIDO.*
FROM COMPOSTEIRAS
RIGHT JOIN COMPOSTO_LIQUIDO ON COMPOSTEIRAS.idComposteira = COMPOSTO_LIQUIDO.idComposteira;

--Consulta com FULL OUTER JOIN

SELECT *
FROM COMPOSTEIRAS
FULL OUTER JOIN COMPOSTO_LIQUIDO ON COMPOSTEIRAS.idComposteira = COMPOSTO_LIQUIDO.idComposteira
FULL OUTER JOIN COMPOSTO_HUMUS ON COMPOSTEIRAS.idComposteira = COMPOSTO_HUMUS.idComposteira;

--Consulta com CROSS JOIN:

SELECT MORADORES.nome, COMPOSTEIRAS.idComposteira
FROM MORADORES
CROSS JOIN COMPOSTEIRAS;
